{
  "name": "BreathWatch",
  "version": "1.0",
  "description": "Reminder to watch your breath and become more mindful throughout your day",
  "permissions": ["storage", "alarms", "notifications"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html"
  },
  "browser_action": {
    "default_popup": "options.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
